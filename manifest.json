{
  "manifest_version": 3,
  "name": "AISaver",
  "version": "0.3",
  "permissions": [
    "downloads",
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://chat.openai.com/*",
    "https://chatgpt.com/*",
    "https://dav.jianguoyun.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "options_page": "options.html"
}
